<main class="album-new">
  <nav>
    <img class="logo-nav" src="images/music_logo_2.png" alt="logo Moodzik" />
    <div class="nav-content">
      <a href="/homepage" class="homepage">Homepage</a>
      <a href="/album-new" class="create-album">Create an album</a>
    </div>
    <img class="i-profil" src="/images/femme.png" alt="icÃ´ne profile" />
  </nav>

  <div class="main-album-new">
    <form action="/album-new" method="POST">
      <label for="title">Titre d'album</label>
      <input type="text" name="title" id="title" />

      <div>
        <input type="text" name="track" />
        <button>Add</button>
      </div>

      {{#each tracks}}
        <p>{{name}} - {{artists.name}}</p>
      {{/each}}

      <div class="album-btn-container">
        <button>
          Validate
          <span></span><span></span><span></span><span></span>
        </button>
      </div>
    </form>
  </div>
</main>